
@model Feature
@{
    ViewData["Title"] = "Create";
}

<div class="container-fluid">
    <form method="post">
        <div class="form-row">
            <div class="form-group col-md-6">
                <label asp-for="Title">Title1</label>
                <input asp-for="Title" class="form-control" placeholder="Title1">
            </div>
            <div class="form-group col-md-6">
                <label asp-for="Subtitle">SubTitle</label>
                <input asp-for="Subtitle" class="form-control" placeholder="SubTitle">
            </div>
        </div>
        <div class="form-row">

            <div class="form-group col-md-4">
                <label asp-for="Icon">RedirectUrl</label>
                <input asp-for="Icon" class="form-control" placeholder="Icon">
            </div>


        </div>
        <div class="form-group">
            <button type="submit" class="btn btn-primary">Edit</button>
        </div>
    </form>
</div>




